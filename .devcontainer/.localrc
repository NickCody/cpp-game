function fssh() {
    killall ssh-agent
    ssh-agent > ~/.ssh-agent
    source ~/.ssh-agent
    ssh-add ~/.ssh/id_rsa
    ssh-add ~/.ssh/google_compute_engine
}

export -f fssh

