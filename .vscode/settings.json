{
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.allowChords": false,
  "bazel-cpp-tools.compileCommands.targets": [
    "//...",
  ],
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "clang-format.executable": "/usr/bin/clang-format",
  "C_Cpp.default.intelliSenseMode": "linux-clang-x64",
  "terminal.integrated.scrollback": 100000,
  "editor.detectIndentation": false,
  "yaml.schemas": {
    "pmud/schema/items-schema.json": "items.yml",
    "pmud/schema/monsters-schema.json": "monsters.yml",
    "pmud/schema/places-schema.json": "places.yml"
  },
  "files.associations": {
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "condition_variable": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "regex": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "fstream": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "ranges": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "stop_token": "cpp",
    "streambuf": "cpp",
    "thread": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "variant": "cpp",
    "any": "cpp",
    "cfenv": "cpp",
    "charconv": "cpp",
    "cinttypes": "cpp",
    "codecvt": "cpp",
    "complex": "cpp",
    "coroutine": "cpp",
    "csetjmp": "cpp",
    "cuchar": "cpp",
    "netfwd": "cpp",
    "rope": "cpp",
    "slist": "cpp",
    "future": "cpp",
    "numbers": "cpp",
    "scoped_allocator": "cpp",
    "shared_mutex": "cpp",
    "span": "cpp",
    "valarray": "cpp"
  },
  "files.autoSave": "onFocusChange",
  "Lua.diagnostics.globals": [
    "redis"
  ],
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "fileHeaderComment.parameter": {
    "*": {
      "commentbegin": "/**",
      "commentprefix": " *",
      "commentend": " */",
      "organization": "Primordia"
    }
  },
  "fileHeaderComment.template": {
    "file": [
      "${commentbegin}",
      // "${commentbegin} -=-=--=-===--=-=-=-=-=-==--=-==-==--=-=-====-=-=-=-=-=-=-=-=-=-=-=-=---=-==",
      "${commentprefix}  ${filename}",
      "${commentprefix}",
      "${commentprefix}  Created on ${date}",
      // "${commentprefix}  =-=-====-=-=-=-=-=-=-=-=-=-=-=-=--=-=-==--=-=-===-=-=-=-=-=--=-===-==-==-==",
      "${commentend}"
    ],
    "*": [
      "  ${commentbegin}",
      // "  ${commentbegin} -=-=--=-===--=-=-=-=-=-==--=-==-==--=-=-====-=-=-=-=-=-=-=-=-=-=-=-=--",
      "  ${commentprefix} \\brief Brief Description",
      "  ${commentprefix} ",
      "  ${commentprefix} Longer Description",
      "  ${commentprefix} ",
      "  ${commentprefix} @param dummy This is a parameter",
      "  ${commentprefix} @return The test results",
      "  ${commentprefix}",
      "  ${commentend}"
    ],
  },
  "C_Cpp.intelliSenseEngine": "Disabled",
  "C_Cpp.autocomplete": "Disabled",
  "C_Cpp.errorSquiggles": "Disabled",
  "bsv.bzl.lsp.enableCodelenses": true,
  "bsv.bzl.lsp.enableCodelensCopyLabel": true,
  "bsv.bzl.lsp.enableCodelensCodesearch": true,
  "bsv.bzl.lsp.enableCodelensBrowse": true,
  "bsv.bzl.lsp.enableCodelensStarlarkDebug": true,
  "bsv.bzl.lsp.enableCodelensBuild": true,
  "bsv.bzl.lsp.enableCodelensTest": true,
  "bsv.bzl.lsp.enableCodelensRun": true,
  "[bazel]": {
    "editor.formatOnSave": true
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "googletest.failed",
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "scope": "googletest.passed",
        "settings": {
          "foreground": "#0f0"
        }
      },
      {
        "scope": "googletest.run",
        "settings": {
          "foreground": "#0f0"
        }
      }
    ]
  },
  "testMate.cpp.test.advancedExecutables": [
    {
      "name": "non-actor-debug",
      "pattern": "./bazel-out/k8-dbg/**/*test.runfiles/**/non-actor/*test",
      "cwd": "${workspaceDirectory}"
    },
    {
      "name": "non-actor-fast",
      "pattern": "./bazel-out/k8-fastbuild/**/*test.runfiles/**/*test",
      "cwd": "${workspaceDirectory}"
    },
    {
      "name": "actor",
      "pattern": "./bazel-out/k8-dbg/**/actor/*test.runfiles/**/*test",
      "cwd": "${workspaceDirectory}"
    }
  ],
  "testMate.cpp.test.parallelExecutionLimit": 12,
  "testExplorer.useNativeTesting": true,
  "bsv.bzl.server.address": "http://127.0.0.1:8080",
}